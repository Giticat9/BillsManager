{
	"printWidth": 120,
	"tabWidth": 4,
	"useTabs": true,
	"semi": true,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"trailingComma": "none",
	"arrowParens": "avoid",
	"bracketSpacing": true,
	"quoteProps": "consistent",
	"proseWrap": "preserve",
	"htmlWhitespaceSensitivity": "strict",
	"vueIndentScriptAndStyle": true,
	"endOfLine": "lf",
	"singleAttributePerLine": true,
	"bracketSameLine": false,
	"embeddedLanguageFormatting": "auto",
	"insertPragma": false,
	"requirePragma": false
}
